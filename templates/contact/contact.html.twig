{% extends 'base.html.twig' %}

{% block title %}Hello ContactController!
{% endblock %}

{% block body %}
	{% for type, messages in app.flashes(['success', 'warning', 'info' , 'danger']) %}
		{% for message in messages %}
			<div class="alert alert-{{ type }}">
				{{ message }}
			</div>
		{% endfor %}
	{% endfor %}
	{{ form(form) }}{% endblock %}
